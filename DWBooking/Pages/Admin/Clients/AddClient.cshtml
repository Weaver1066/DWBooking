@page
@model DWBooking.Pages.Admin.Clients.AddClientModel
@{
}

<div class="page-breadcrumb">
    <div class="row">
        <div class="col-5">
            <h4 class="page-title" style="text-align: left;">Book Vejledning</h4>
        </div>
        <div class="col-7 align-self-center">
            <div class="d-flex align-items-center justify-content-end">
                <nav aria-label="breadcrumb">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item">
                            <a href="#">Home</a>
                        </li>
                        <li class="breadcrumb-item active" aria-current="page">Book Vejledning</li>
                    </ol>
                </nav>
            </div>
        </div>
    </div>
</div>
<div class="container-fluid">
    <div class="row">
        <div class="col-lg-12">
            <div class="card card-body">
                <form class="form-horizontal mt-4">
                    <div class="form-group">
                        <div class="row">
                            <label>Søg<span class="help"></span></label>
                            <div class="col-lg-11">
                                <input type="text" class="form-control" value="">
                            </div>
                            <div class="col-lg-1">
                                <button class="btn btn-primary form-control">Søg</button>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <div class="card card-body">
                <form class="form-horizontal mt-4">
                    <div class="form-group">
                        <label class="text-left">Navn<span class="help"></span></label>
                        <input type="text" class="form-control" placeholder="George deo...">
                    </div>
                    <div class="form-group">
                        <label class="form-label" for="example-email">
                            Email
                            <span class="help">
                            </span>
                        </label>
                        <input type="email" id="example-email" name="example-email" class="form-control"
                               placeholder="Email">
                    </div>
                    <div class="form-group">
                        <label class="form-label">Adresse<span class="help"></span></label>
                        <input type="text" class="form-control" placeholder="Adresse">
                    </div>
                    <div class="form-group">
                        <label>Telefon<span class="help"></span></label>
                        <input type="text" class="form-control" placeholder="Telefon">
                    </div>
                    <div class="form-group">
                        <label class="form-label">Dato<span class="help"></span></label>
                        <input type="date" class="form-control" placeholder="">
                    </div>
                    <div class="form-group">
                        <label class="form-label">Tid<span class="help"></span></label>
                        <input type="datetime" class="form-control">
                    </div>
                    <div class="form-group">
                        <button class="btn btn-primary form-control">Opret</button>
                    </div>

                </form>
            </div>
        </div>
    </div>
</div>
